[include]
  path = .basegitconfig
[includeIf "gitdir:~/Developments/work/"]
  path = .workgitconfig 
[alias]
	ac = commit -am
[merge]
	conflictstyle = diff3
[filter "lfs"]
	clean = git-lfs clean -- %f
	smudge = git-lfs smudge -- %f
	process = git-lfs filter-process
	required = true
[credential]
	helper = 
	helper = /usr/local/share/gcm-core/git-credential-manager
[credential "https://dev.azure.com"]
	useHttpPath = true
